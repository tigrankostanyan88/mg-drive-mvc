<div class="modal fade" id="signIn_signUp" tabindex="-1" aria-labelledby="signIn_signUp" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content rounded-4 shadow-lg">
      <div class="modal-header">
        <h5 class="modal-title" id="loginModalLabel">Մուտք / Գրանցում</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Փակել"></button>
      </div>
      <div class="modal-body">
        <ul class="nav nav-tabs mb-5" id="myTab" role="tablist">
          <li class="nav-item" role="presentation">
            <button class="nav-link active px-5 text-primary" data-bs-toggle="tab" data-bs-target="#signIn"
              type="button" role="tab" aria-controls="signIn" aria-selected="true">Մուտք</button>
          </li>
          <li class="nav-item" role="presentation">
            <button class="nav-link px-5 text-primary" data-bs-toggle="tab" data-bs-target="#signUp"
              type="button" role="tab" aria-controls="signUp" aria-selected="false">Գրանցում</button>
          </li>
        </ul>
        <!-- ocntent modal -->
        <div class="tab-content" id="myTabContent">
          <div class="tab-pane fade show active" id="signIn" role="tabpanel" aria-labelledby="signIn"
            tabindex="0">
           <%- include('./signIn-form.ejs') %>
          </div>
          <div class="tab-pane fade" id="signUp" role="tabpanel" aria-labelledby="signUp" tabindex="0">
            <%- include('./signUp-form.ejs') %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>