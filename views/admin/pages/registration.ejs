<!doctype html>
<html lang="en">
<head><%- include('../parts/head.ejs') %></head>
<body>
    <div class="container-scroller">
        <%- include('../parts/sidebar.ejs') %>
        <div class="container-fluid page-body-wrapper">
            <%- include('../parts/nav.ejs') %>
            <!-- Main panel -->
            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="row ">
                        <div class="col-12 grid-margin">
                            <div class="card">
                                <div class="card-body">
                                    <h4 class="card-title">Գրանցված օգտատերեր</h4>
                                    <div class="table-responsive">
                                        <table class="table">
                                            <thead>
                                                <tr>
                                                    <th> Անունը </th>
                                                    <th> Ազգանունը </th>
                                                    <th> Էլ․ հասցե </th>
                                                    <th> Հեռախոսահամար </th>
                                                    <th> Գրանցման ամսաթիվը </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                               <% registedUser.forEach(user => { %>
                                                     <tr>
                                                        <td><span class="pl-2"><%- user.name %></span></td>
                                                        <td><span class="pl-2"><%- user.lastname %></span></td>
                                                        <td><span class="pl-2"><%- user.email %></span></td>
                                                        <td><span class="pl-2"><%- user.phone %></span></td>
                                                        <td><span class="pl-2"><%- helpers.getDate(user.createdAt) %></span></td>
                                                     </tr>
                                                <% }); %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <%- include('../parts/footer.ejs') %>
            </div>
        </div>
    </div>
    <%- include('../parts/scripts.ejs') %>
</body>

</html>